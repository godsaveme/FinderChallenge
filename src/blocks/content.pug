    function CreateItems(books){
        var booksRow = window.document.getElementById("booksRow");
        for (var i = 0; i < books.length; i++) {
            var div = document.createElement("DIV");
            //var txt = document.createTextNode("Thirds");
            div.className = "pure-u-1-3";
            //div.appendChild(txt);
            div.innerHTML = '<img src='+books[i].image+'>';
            booksRow.appendChild(div);
        }
    }